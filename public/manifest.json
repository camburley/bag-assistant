{
    "manifest_version": 2,
    "name": "Bag Assistant",
    "version": "1.0",
    "permissions": ["http://localhost:5005/api/v1/assistant/completion"],
    "browser_action": {
      "default_popup": "index.html",
      "default_icon": "icon.png"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
  }